{
  "name": "nomad-tribe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "initialize": "bash initialize.sh",
    "start": "babel-node server/app.js",
    "clean": "rimraf node_modules && rimraf dist && npm install",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build": "rimraf dist && npm run webpack:dev && npm run start",
    "build:prod": "rimraf dist && npm run webpack:prod && npm run start",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "lint": "eslint client server; exit 0"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bookshelf": "^0.14.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^5.0.0-alpha.7",
    "helmet": "^3.18.0",
    "http-status-codes": "^1.3.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.16.5",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^3.2.9",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  }
}
